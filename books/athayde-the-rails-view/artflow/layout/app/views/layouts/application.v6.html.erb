<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Artflow</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/header' %>
  <%= image_tag("tagline.png",
                alt: "Creative File Management Made Easy",
                id: "tagline") %>
  <div class="container">
    <%# START_HIGHLIGHT %>
    <section id="content">
      <%= yield %>
    </section>
    <section id="sidebar">
    </section>
    <%# END_HIGHLIGHT %>
  </div>
  <%= render 'layouts/footer' %>
</body>
</html>
